using System;

class Program
{
    static void Main(string[] args)
    {
        Console.Write("Ingresa un n√∫mero: ");
        int numero = Convert.ToInt32(Console.ReadLine());

        for (int i = 0; i < numero; i++)
        {
            for (int j = 0; j < numero; j++)
            {
                Console.Write("**");
            }
            Console.WriteLine(); 
        }
    }
}
