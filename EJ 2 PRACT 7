using System;

class Program
{
    static void Main(string[] args)
    {
        Console.WriteLine("NÃºmeros divisibles entre 3 del 1 al 1000:");

        int contador = 1;

        for (int i = 1; i <= 1000; i++)
        {
            if (i % 3 == 0)
            {
                Console.WriteLine($"Divisor de Nataly Salinas {contador}: {i}");
                contador++;
            }
        }
    }
}
